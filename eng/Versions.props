    <SystemDirectoryServicesVersion>5.0.0-alpha1.19368.4</SystemDirectoryServicesVersion>
    <SystemDrawingCommonVersion>5.0.0-alpha1.19368.4</SystemDrawingCommonVersion>
    <SystemIOFileSystemAccessControlVersion>5.0.0-alpha1.19368.4</SystemIOFileSystemAccessControlVersion>
    <SystemIOPackagingVersion>5.0.0-alpha1.19368.4</SystemIOPackagingVersion>
    <SystemResourcesExtensionsPackageVersion>5.0.0-alpha1.19368.4</SystemResourcesExtensionsPackageVersion>
    <SystemSecurityAccessControlVersion>5.0.0-alpha1.19368.4</SystemSecurityAccessControlVersion>
    <SystemSecurityCryptographyCngVersion>5.0.0-alpha1.19368.4</SystemSecurityCryptographyCngVersion>
    <SystemSecurityCryptographyPkcsVersion>5.0.0-alpha1.19368.4</SystemSecurityCryptographyPkcsVersion>
