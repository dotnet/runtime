<configuration>
  <!-- Don't use any higher level config files.
       Our builds need to be isolated from user/machine state -->
  <fallbackPackageFolders>
    <clear />
  </fallbackPackageFolders>
  <packageSources>
    <clear />
    <!-- this feed upstreams to the other feeds -->
    <add key="dotnet-runtime-CI" value="https://pkgs.dev.azure.com/dnceng/_packaging/dotnet-runtime-CI/nuget/v3/index.json" />
  </packageSources>
  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
</configuration>
