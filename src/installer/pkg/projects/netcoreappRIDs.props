      <Platform>s390x</Platform>
    </UnofficialBuildRID>
    <UnofficialBuildRID Include="linux-ppc64le">
      <Platform>ppc64le</Platform>
    </UnofficialBuildRID>
    <UnofficialBuildRID Include="linux-musl-ppc64le">
