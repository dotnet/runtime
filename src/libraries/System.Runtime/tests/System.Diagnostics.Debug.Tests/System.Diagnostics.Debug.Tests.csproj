    <Compile Include="DebuggerDisplayAttributeTests.cs" />
    <Compile Include="DebuggerTests.cs" />
    <Compile Include="DebuggerTypeProxyAttributeTests.cs" />
    <Compile Include="DebuggerVisualizerAttributeTests.cs" />
