    <!-- linux mint includes major version in Include because Parent is different for each major -->
    <RuntimeGroup Include="linuxmint.19">
      <Parent>ubuntu.18.04</Parent>
      <Architectures>x64</Architectures>
      <Versions>1;2</Versions>
    </RuntimeGroup>

    <RuntimeGroup Include="miraclelinux">
      <Parent>rhel</Parent>
      <Architectures>x64</Architectures>
      <Versions>8;9</Versions>
      <ApplyVersionsToParent>true</ApplyVersionsToParent>
      <TreatVersionsAsCompatible>false</TreatVersionsAsCompatible>
    </RuntimeGroup>

    <RuntimeGroup Include="ol">
      <Parent>rhel</Parent>
      <Architectures>x64</Architectures>
      <Versions>7;8</Versions>
      <TreatVersionsAsCompatible>false</TreatVersionsAsCompatible>
      <ApplyVersionsToParent>true</ApplyVersionsToParent>
    </RuntimeGroup>
    <!-- The host doesn't strip minors for Oracle Linux.
         We map them here to the major version or Oracle Linux and RHEL. -->
    <RuntimeGroup Include="ol.7">
      <Parent>rhel.7</Parent>
      <Architectures>x64</Architectures>
      <Versions>0;1;2;3;4;6;6</Versions>
      <ApplyVersionsToParent>false</ApplyVersionsToParent>
      <OmitRIDDefinitions>ol;ol.7;ol.7-x64</OmitRIDDefinitions>
    </RuntimeGroup>
    <RuntimeGroup Include="ol.8">
      <Parent>rhel.8</Parent>
      <Architectures>x64</Architectures>
      <Versions>0</Versions>
      <ApplyVersionsToParent>false</ApplyVersionsToParent>
      <OmitRIDDefinitions>ol;ol.8;ol.8-x64</OmitRIDDefinitions>
    </RuntimeGroup>

    <!-- all opensuse releases are independent -->
    <RuntimeGroup Include="opensuse">
      <Parent>linux</Parent>
      <Architectures>x64</Architectures>
      <TreatVersionsAsCompatible>false</TreatVersionsAsCompatible>
    </RuntimeGroup>

    <RuntimeGroup Include="osx">
      <Parent>unix</Parent>
      <Architectures>x64;arm64</Architectures>
