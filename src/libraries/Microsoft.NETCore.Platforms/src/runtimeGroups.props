    </RuntimeGroup>

    <RuntimeGroup Include="smartos">
      <Parent>illumos</Parent>
      <Architectures>x64</Architectures>
      <Versions>2020;2021</Versions>
    </RuntimeGroup>

    <!-- rhel 6 is independent -->
    <RuntimeGroup Include="rhel">
      <Parent>linux</Parent>
      <Architectures>x64</Architectures>
      <Versions>6</Versions>
    </RuntimeGroup>

    <RuntimeGroup Include="rhel">
      <Parent>linux</Parent>
      <Architectures>x64</Architectures>
      <Versions>7</Versions>
    </RuntimeGroup>
    <RuntimeGroup Include="rhel">
      <Parent>linux</Parent>
      <Architectures>x64;arm64</Architectures>
