    </RuntimeGroup>
    <RuntimeGroup Include="rhel">
      <Parent>linux</Parent>
      <Architectures>x64;arm64</Architectures>
      <Versions>9</Versions>
    </RuntimeGroup>

    <RuntimeGroup Include="rocky">
