<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <!-- Use live ILLink so that the assembly gets IsAotCompatible and IsTrimmable metadata despite
         not targeting a TFM supported by the shipping trimming tooling. -->
    <UseLiveILLink>true</UseLiveILLink>
    <Nullable>disable</Nullable>
    <Win32Resource>EventLogMessages.res</Win32Resource>
    <!-- Override the parent Directory.Build.props -->
    <SupportedOSPlatforms />
    <EnableLibraryImportGenerator>false</EnableLibraryImportGenerator>
    <SkipIncludePolyfills>true</SkipIncludePolyfills>
    <SkipIncludeNullableAttributes>true</SkipIncludeNullableAttributes>
  </PropertyGroup>
</Project>