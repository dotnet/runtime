    <IgnoreForCI Condition="'$(TargetOS)' == 'browser'">true</IgnoreForCI>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="ServicePointManagerTest.cs" />
    <Compile Include="TlsSystemDefault.cs" />
  </ItemGroup>
</Project>
