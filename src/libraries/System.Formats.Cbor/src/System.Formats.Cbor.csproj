<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Nullable>enable</Nullable>
    <AssemblyName>System.Formats.Cbor</AssemblyName>
    <TargetFrameworks>$(NetCoreAppCurrent)</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="System\Formats\Cbor\CborReader.SkipValue.cs" />
    <Compile Include="System\Formats\Cbor\CborReader.Simple.cs" />
    <Compile Include="System\Formats\Cbor\CborReader.Tag.cs" />
    <Compile Include="System\Formats\Cbor\CborTag.cs" />
    <Compile Include="System\Formats\Cbor\CborInitialByte.cs" />
    <Compile Include="System\Formats\Cbor\CborReader.Array.cs" />
    <Compile Include="System\Formats\Cbor\CborReader.cs" />
    <Compile Include="System\Formats\Cbor\CborReader.Integer.cs" />
    <Compile Include="System\Formats\Cbor\CborReader.Map.cs" />
    <Compile Include="System\Formats\Cbor\CborReader.String.cs" />
    <Compile Include="System\Formats\Cbor\CborWriter.Array.cs" />
    <Compile Include="System\Formats\Cbor\CborWriter.Map.cs" />
    <Compile Include="System\Formats\Cbor\CborWriter.Simple.cs" />
    <Compile Include="System\Formats\Cbor\CborWriter.String.cs" />
