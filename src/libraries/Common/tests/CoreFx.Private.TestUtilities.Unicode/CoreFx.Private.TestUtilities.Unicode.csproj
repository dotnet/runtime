    <Compile Include="System\Text\Unicode\PropsFileEntry.cs" />
    <Compile Include="System\Text\Unicode\ParsedUnicodeData.cs" />
    <Compile Include="System\Text\Unicode\CodePointFlags.cs" />
    <Compile Include="System\Text\Unicode\GraphemeClusterBreakProperty.cs" />
    <Compile Include="System\Text\Unicode\BidiClass.cs" />
    <Compile Include="System\Text\Unicode\CodePoint.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="$(CommonTestPath)Data\CaseFolding-12.1.0.txt">
      <Link>UnicodeData\CaseFolding-12.1.0.txt</Link>
      <LogicalName>CaseFolding-12.1.0.txt</LogicalName>
    </EmbeddedResource>
    <EmbeddedResource Include="$(CommonTestPath)Data\DerivedBidiClass-12.1.0.txt">
      <Link>UnicodeData\DerivedBidiClass-12.1.0.txt</Link>
      <LogicalName>DerivedBidiClass-12.1.0.txt</LogicalName>
    </EmbeddedResource>
    <EmbeddedResource Include="$(CommonTestPath)Data\DerivedName-12.1.0.txt">
      <Link>UnicodeData\DerivedName-12.1.0.txt</Link>
      <LogicalName>DerivedName-12.1.0.txt</LogicalName>
