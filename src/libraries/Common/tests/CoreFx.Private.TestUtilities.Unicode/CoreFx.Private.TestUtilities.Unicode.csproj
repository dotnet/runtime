<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Configurations>netcoreapp-Debug;netcoreapp-Release</Configurations>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="System\Text\Unicode\UnicodeData.cs" />
