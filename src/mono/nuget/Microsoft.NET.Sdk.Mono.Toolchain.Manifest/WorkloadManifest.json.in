      "kind": "Sdk",
      "version": "${PackageVersion}",
      "alias-to": {
        "osx-x64": "microsoft.netcore.app.runtime.aot.osx-x64.cross.tvos-x64",
        "osx-arm64": "microsoft.netcore.app.runtime.aot.osx-x64.cross.tvos-x64"
