      <Compile Include="$(BclSourcesRoot)\System\RuntimeType.Mono.cs" />
      <Compile Include="$(BclSourcesRoot)\System\RuntimeTypeHandle.cs" />
      <Compile Include="$(BclSourcesRoot)\System\String.Mono.cs" />
