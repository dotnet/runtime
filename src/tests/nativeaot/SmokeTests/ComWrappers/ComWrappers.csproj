  </PropertyGroup>
  <ItemGroup>
    <Compile Include="ComWrappers.cs" />
  </ItemGroup>

  <ItemGroup>
    <CMakeProjectReference Include="CMakeLists.txt" />
  </ItemGroup>
</Project>
