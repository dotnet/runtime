<Project Sdk="Microsoft.NET.Sdk.IL">
  <PropertyGroup>
    <!-- Needed for issues.target - need to prevent Mono Ahead of Time compiler
         from running -->
    <RequiresProcessIsolation>true</RequiresProcessIsolation>
    <!-- Test checks for an issue that cannot occur with the interpreter  -->
    <InterpreterIncompatible>true</InterpreterIncompatible>
    <DebugType>PdbOnly</DebugType>
    <Optimize>True</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Test_HndIndex_10_Reordered.il" />
  </ItemGroup>
</Project>
