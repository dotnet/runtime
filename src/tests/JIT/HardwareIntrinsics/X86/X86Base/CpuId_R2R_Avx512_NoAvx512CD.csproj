<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>X86_CpuId_R2R_Avx512_NoAvx512CD</AssemblyName>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup>
    <AlwaysUseCrossGen2>true</AlwaysUseCrossGen2>
    <IlasmRoundTripIncompatible>true</IlasmRoundTripIncompatible>
    <NativeAotIncompatible>true</NativeAotIncompatible>
  </PropertyGroup>

  <ItemGroup>
    <CrossGen2TestExtraArguments Include="$(CrossGen2TestExtraArguments) --instruction-set:avx512f,avx512f_vl,avx512bw,avx512bw_vl,-avx512cd,avx512cd_vl,avx512dq,avx512dq_vl" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="CpuId.cs" />
  </ItemGroup>
</Project>
