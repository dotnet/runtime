<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>X86_X86Base_ro</AssemblyName>
    <BuildAsStandalone>false</BuildAsStandalone>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <!-- https://github.com/dotnet/runtime/issues/73454;https://github.com/dotnet/runtime/pull/61707#issuecomment-973122341 -->
    <MonoAotIncompatible>true</MonoAotIncompatible>
  </PropertyGroup>
  <PropertyGroup>
    <DebugType>Embedded</DebugType>
    <Optimize>True</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="DivRem.Int32.Tuple3Op.cs" />
    <Compile Include="DivRem.UInt32.Tuple3Op.cs" />
    <Compile Include="DivRem.nint.Tuple3Op.cs" />
    <Compile Include="DivRem.nuint.Tuple3Op.cs" />
    <Compile Include="Program.X86Base.cs" />
    <Compile Include="..\Shared\Program.cs" />
  </ItemGroup>
</Project>
