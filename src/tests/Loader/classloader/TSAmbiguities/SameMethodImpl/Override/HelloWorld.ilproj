<Project Sdk="Microsoft.NET.Sdk.IL">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <!-- This test requires explicit Main because modifying visibilities -->
    <!-- to satisfy the wrapper generator affects test behavior. -->
    <RequiresProcessIsolation>true</RequiresProcessIsolation>
    <ReferenceXUnitWrapperGenerator>false</ReferenceXUnitWrapperGenerator>
    <!--https://github.com/dotnet/runtimelab/issues/155: Thorough checking of invalid inputs-->
    <NativeAotIncompatible>true</NativeAotIncompatible>
    <!-- need triage -->
    <DisableProjectBuild Condition="'$(RuntimeFlavor)' == 'Mono'">true</DisableProjectBuild>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="HelloWorld.il" />
  </ItemGroup>
</Project>
