<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!-- This test requires process isolation as it monitors the number of JITted methods. -->
    <RequiresProcessIsolation>true</RequiresProcessIsolation>
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
    <JitOptimizationSensitive>true</JitOptimizationSensitive>
    <InterpreterIncompatible>true</InterpreterIncompatible> <!-- This test is too sensitive to exactly the behavior of the jit that even thought it basically works in some interpreter modes its just not ready to be used in any interpreter scenarios -->
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="JittedMethodsCountingTest.cs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="$(TestLibraryProjectPath)" />
  </ItemGroup>

</Project>
