        "versionSpec": "1.*",
        "definitionType": "task"
      },
      "inputs": {
        "filename": "docker",
        "arguments": "exec $(PB_DockerContainerName) git clone $(PB_RepoUrl) $(PB_GitDirectory)",
        "workingFolder": "",
        "failOnStandardError": "false"
      }
    },
    {
