<Project>

  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory).., Directory.Build.props))\Directory.Build.props" />

  <!-- Some signing stages check Build.props to find which projects to depend on. -->
  <Import Project="$(RepositoryEngineeringDir)Build.props" />

  <PropertyGroup>
    <TargetFramework>$(NETCoreAppFramework)</TargetFramework>

    <!-- Skip signing steps by default for non-official builds. -->
    <SkipSigning Condition="'$(SkipSigning)' == '' and '$(OfficialBuild)' != 'true'">true</SkipSigning>
  </PropertyGroup>

</Project>
