<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>$(NetCoreAppToolCurrent)</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsShipping>false</IsShipping>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="$(RepoRoot)\src\native\managed\cdacreader\src\Constants.cs" Link="CDAC\Constants.cs" />
    <Compile Include="$(RepoRoot)\src\native\managed\cdacreader\src\ContractDescriptorParser.cs" Link="CDAC\ContractDescriptorParser.cs" />
    <Compile Include="$(RepoRoot)\src\native\managed\cdacreader\src\Contracts\*.cs" Link="CDAC\Contracts\%(FileName).cs" />
    <Compile Include="$(RepoRoot)\src\native\managed\cdacreader\src\DataType.cs" Link="CDAC\DataType.cs" />
    <Compile Include="$(RepoRoot)\src\native\managed\cdacreader\src\Data\*.cs" Link="CDAC\Data\%(FileName).cs" />
    <Compile Include="$(RepoRoot)\src\native\managed\cdacreader\src\Root.cs" Link="CDAC\Root.cs" />
    <Compile Include="$(RepoRoot)\src\native\managed\cdacreader\src\Target.cs" Link="CDAC\Target.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.CommandLine" Version="$(SystemCommandLineVersion)" />
  </ItemGroup>
</Project>
