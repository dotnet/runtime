private const string SimpleOpTest_ValidationLogic = @"if ({ValidateFirstResult})
            {
                succeeded = false;
            }
            else
            {
                for (var i = 1; i < RetElementCount; i++)
                {
                    if ({ValidateRemainingResults})
                    {
                        succeeded = false;
                        break;
                    }
                }
            }";

private const string SimpleVecOpTest_ValidationLogic = @"for (var i = 0; i < RetElementCount; i++)
            {
                if ({ValidateIterResult})
                {
                    succeeded = false;
                    break;
                }
            }";

private const string VecPairBinOpTest_ValidationLogic = @"
            int index = 0;
            int half  = RetElementCount / 2;
            for (var i = 0; i < RetElementCount; i+=2, index++)
            {
                    if ({ValidateEntry})
                    {
                        succeeded = false;
                        break;
                    }
            }";

private const string SimpleVecIterOpTest_ValidationLogic = @"{RetBaseType} temp = 0;
            for (var i = 0; i < RetElementCount; i++)
            {
                {CalculateIterResults}
            }
            succeeded = {CheckFinalResults};";

