        "versionSpec": "1.*",
        "definitionType": "task"
      },
      "inputs": {
        "filename": "$(Agent.BuildDirectory)/s/build.sh",
        "arguments": "$(PB_BuildType) $(Architecture) skipnuget -skiprestore stripSymbols -OfficialBuildId=$(OfficialBuildId) -clang6.0",
        "workingFolder": "",
        "failOnStandardError": "false"
      }
