    /*
     * Open a method, and also provide its real section offset in image
     */
    HRESULT OpenMethod2( [in] mdMethodDef method,
                        [in] ULONG32 isect,
                        [in] ULONG32 offset);

    /*
     * Commit the changes written so far to the stream
     */

    HRESULT Commit();
};

