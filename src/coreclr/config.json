    "RestoreDuringBuild": {
      "description": "Enables/disables package restore.",
      "valueType": "property",
      "values": [ true, false ],
      "defaultValue": true
    },
    "Project": {
      "description": "Project where the commands are going to be applied.",
      "valueType": "passThrough",
      "values": [],
      "defaultValue": ""
